{
  "name": "billbeesley",
  "version": "1.0.0",
  "description": "My personal resume",
  "main": "dist/main/index.js",
  "repository": "https://github.com/bbeesley/billbeesley",
  "author": "Bill Beesley <bill@billbeesley.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist browser",
    "compile": "cross-env BABEL_ENV=production webpack --config webpack.config.dist.js",
    "flow": "flow",
    "flow:delete": "find ./src/main -name *.flow -delete",
    "flow:generate": "flow-copy-source -v ./src/main ./dist/main",
    "lint": "./node_modules/.bin/eslint src/**/*.js --fix",
    "mocha": "cross-env BABEL_ENV=test mocha --require ./src/test/index.js \"src/test/unit/**/*.@(js|jsx)\"",
    "start": "cross-env SITE_DEFAULT=true BABEL_ENV=browser webpack-dev-server --config webpack.config.dev.js",
    "pretest": "run-p clean lint flow",
    "test": "cross-env BABEL_ENV=test nyc npm run mocha",
    "posttest": "npm run compile"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-syntax-flow": "^7.0.0-beta.44",
