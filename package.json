  "scripts": {
    "clean": "rm -rf dist browser",
    "compile": "BABEL_ENV=production webpack --config webpack.config.dist.js",
    "flow": "flow",
    "flow:delete": "find ./src/main -name *.flow -delete",
    "flow:generate": "flow-copy-source -v ./src/main ./dist/main",
    "lint": "./node_modules/.bin/eslint src/**/*.js --fix",
    "mocha": "#BABEL_ENV=test mocha --require ./src/test/index.js \"src/test/unit/**/*.@(js|jsx)\"",
    "start": "SITE_DEFAULT=true BABEL_ENV=browser webpack-dev-server --config webpack.config.dev.js",
    "pretest": "run-p clean lint flow",
    "test": "BABEL_ENV=test nyc npm run mocha",
    "posttest": "npm run compile"
  },
  "devDependencies": {
